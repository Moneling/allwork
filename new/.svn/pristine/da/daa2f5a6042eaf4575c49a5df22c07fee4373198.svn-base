@charset "UTF-8";
@imposrt url(../common-css/reset.css);
.ui-jqgrid-htable tr.ui-jqgrid-labels th {
		white-space: normal !important;
		height:auto;
		vertical-align:text-top;
		padding-top:-12px;
		}
.ui-jqgrid tr.jqgrow td {
		white-space: normal !important;
		height:auto;
		vertical-align:text-top;
		padding-top:2px;
		}
body>.ui-widget{
	position: absolute;
    top: 50% !important;
    left: 50% !important;
    transform: translate(-50%, -50%) !important; 
}
.page-header h1{
	text-align:left !important;
}
.stdform label {
    float: left;
    width: 200px;
    text-align: right;
    padding: 5px 20px 0 0;
}
.popup_material{
	button{
		margin-top:5px;
		font-weight:bold;
	}
}
.header_bg{
	background:#EFF4FA !important;
	border:1px solid #E4E4E4;
}
.basic_info{
	h2{
		border-bottom:1px solid #669fc7;
		font-weight:bold;
		color:#669fc7;
		clear:both;
	}
}
.info_name{
	span{
		text-align:right;
	}
	i{
		display:none;
	}
}
.delete_div{
	position: absolute;
    top: 50% !important;
    left: 50% !important;
    transform: translate(-50%, -50%) !important; 
    z-index:1050;
    display:none;
}
.refresh_div{
	position: absolute;
    top: 50% !important;
    left: 50% !important;
    transform: translate(-50%, -50%) !important; 
    z-index:1050;
    display:none;
}
.content_mask{
	width:100%;
	height:100%;
	background:rgba(0,0,0,.5);
	position:absolute;
	top:0;
	left:0;
	z-index:1049;
}
.prompt_frame{
	position: absolute;
    top: 50% !important;
    left: 50% !important;
    transform: translate(-50%, -50%) !important; 
}
.opsel{
	margin-top:20px;
}
.breadcrumb .btn-radius{
	border:none;
	border-radius: 2px;
}
.ace-videos{
	li{
		border-color: transparent !important;
	}
}
#cboxOverlay{
	background:rgba(0,0,0,.5);
}
.ace-images li{
	border-color: transparent !important;
	padding-top: 11px !important;
}
.ace-images li a{
	width: 84px;
	height:84px;
	text-align: center;
}	
.ace-images li img{
	width: 56px !important;
	height:56px !important;
}
.ace-videos li img{
	width: 56px !important;
	height:56px !important;
	left: 13px !important;
	top: 0.5px !important;
}
.breadcrumb .btn-radius{
	border:none;
	border-radius: 2px;
}
.ace-images li .file-name,.file-name {
    display: block;
	width:84px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	text-align: center;
	padding:0 5px;
}
.row-fluid .ace-videos>li{
	border-color: transparent !important;
}
.row-fluid .ace-videos {
	position: relative;
	zoom: 1;
}
.fileicon {
    position: relative;
    margin: 9px auto 0;
    width: 84px;
    height: 84px;
    background-repeat: no-repeat;
    overflow: hidden;
    text-align:center;
}
.fileicon .playIcon {
    display: none;
    background: url(url(http://s1.pan.bdstatic.com/box-static/disk-system-cdn/widget/pageModule/list/img/video_gridview_arrow_icon_3a7d6a9.png) no-repeat) no-repeat;
    height: 32px;
    width: 32px;
    position: absolute;
    left: 25px;
    top: 26px;
}

.file-name a {
    cursor: pointer;
}
.file-name a,.file-name a:active {
    color: #424e67;
    cursor: default;
    text-decoration: none;
}
.row-fluid .ace-thumbnails>li:hover .playIcon{
	display: block;
}
.row-fluid .ace-thumbnails>li:hover .file-name a {
    color: #06c;
    text-decoration: none;
    outline: 0;
}
#video_mask{
	width:100%;
	height:100%;
	background-color: #000;
	opacity: 0.7;
	position: absolute;
    top: 0;
    left: 0;
    z-index: 1000000;
}
.videoplayer{
	position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    border:15px solid #fff;
    border-radius: 7px;
    background:url(assets/images/video_bg.png) no-repeat;
    background-size: 100% 100%;
    z-index: 1000010;
}
.video_close{
	position: absolute;
	top: -25px;
	right: -25px;
	width:30px;
	height:30px;
	border-radius: 25px;
	background-color: #000;
	opacity: 1;
	border:3px solid #fff; 
	color:#fff;
	text-align: center;
	box-shadow: -1px 1px 2px #000;
	z-index: 10000000;
	float: right;
    font-size: 21px;
    font-weight: bold;
    line-height: 1;
    text-shadow: 0 1px 0 #fff;
}
.ace-thumbnails li:hover{
	background: #f6f8fd;
		border-color: #d3dfec !important;
}
.ace-thumbnails li:hover .video_select{
	display: block;
}
.ace-thumbnails li.active,.ace-thumbnails li.focus{
	background: #eff3f9;
		border-color: #bcccde !important;	
}
.ace-thumbnails li.focus .video_select{
	display: block;
	color: #428bca;
}
.ace-thumbnails li .video_select{
	position: absolute;
	top: 0;
	left: 0;
	width:20px;
	height:20px;
	color:#bcccde; 
	font-size:15px;
	display: none;
	z-index: 100;
}