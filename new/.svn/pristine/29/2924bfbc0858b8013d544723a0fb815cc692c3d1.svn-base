					<script>
						function time(){
							var t=new Date();
						    var M=t.getMonth()+1,
						        d=t.getDate(),
						        h=t.getHours(),
						        m=t.getMinutes(),
						        s=t.getSeconds();
							    M=M<10?'0'+M:M;
							    d=d<10?'0'+d:d;
							    h=h<10?'0'+h:h;
							    m=m<10?'0'+m:m;
							    s=s<10?'0'+s:s;
							 var time=t.getFullYear()+'-'+M+'-'+d+' '+h+':'+m+':'+s;
							 var doc=document.getElementById('clock');
							 doc.innerHTML=time;
						}
						// alert(time());
						// doc.appendChild(time);
						time();
						setInterval(function(){
				          time(),1000  
				        })
					</script>
