@charset 'utf-8';
.mt-3{
	margin-top: 3px;
}
.mt-20{
	margin-top: 20px;
}
/* .ml-20{
	padding-left: -20px;
} */
.border{
	border:1px solid #ccc;
}
.tree-height{
	height: 620px;
	border:1px solid #ccc;
}
small span.logo{
	font-weight: bold;
	font-size: 28px;
	color: #000;
	position: relative;
	top: 20px;
}
.module{
	margin-top: 50px;
	margin-bottom: 0;
}
.module li{
	float: left;
	margin-bottom: 0;
}
.module li a{
	color: #000;
	font-size: 14px;
	margin-left: 10px;
	margin-right: 10px;	
	display: block;
	padding: 5px;
	border-radius: 5px;
}
.module li.on,.module li a:hover{
	background-color: #eb5a57;
}
.contain{
	margin-right: 10px;
}
.contain span{
	position: relative;
	top: 2px;
	color: #d80f0f;
}
.margin{
	margin: 0;
}
.margin li{
	/* border:1px solid #ccc;
	border-right: 2px solid #ccc; */
	padding-left:10px;
	padding-right: 5px;
	height: 20px;
	box-sizing:border-box;
	float: left;
}
.margin li a{
	padding-left: 5px;
	padding-right: 5px;
	background-color: #ccc;
}
.margin li:nth-child(1){
	margin-left: -12px;

}
.margin-b li{
	float: left;
	margin: 0;
}
table{
	margin-top: -10px;
}
table tr td{
	padding-left: 5px;
	padding-right: 5px;
	text-align: center;
}
.page-height{
	height: 600px;
}
.color1{
	color: orange;
	z-index:14444;
	position: absolute;
}